{
  "name": "NHK Ondemand Auto Login",
  "version": "0.1",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "matches": ["https://www.nhk-ondemand.jp/login/login.html"],
      "js": ["login.js"],
      "run_at": "document_end"
    }
  ]
}
